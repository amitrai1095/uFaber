},{"../dotjs/_limit":47,"../dotjs/_limitItems":48,"../dotjs/_limitLength":49,"../dotjs/_limitProperties":50,"../dotjs/allOf":51,"../dotjs/anyOf":52,"../dotjs/dependencies":55,"../dotjs/enum":56,"../dotjs/format":57,"../dotjs/items":58,"../dotjs/multipleOf":59,"../dotjs/not":60,"../dotjs/oneOf":61,"../dotjs/pattern":62,"../dotjs/properties":64,"../dotjs/ref":65,"../dotjs/required":66,"../dotjs/uniqueItems":68,"../dotjs/validate":69}],37:[function(require,module,exports){
'use strict';

/*eslint complexity: 0*/

module.exports = function equal(a, b) {
  if (a === b) return true;

  var arrA = Array.isArray(a)
    , arrB = Array.isArray(b)
    , i;

  if (arrA && arrB) {
    if (a.length != b.length) return false;
    for (i = 0; i < a.length; i++)
      if (!equal(a[i], b[i])) return false;
    return true;
  }

  if (arrA != arrB) return false;

  if (a && b && typeof a === 'object' && typeof b === 'object') {
    var keys = Object.keys(a);
    if (keys.length !== Object.keys(b).length) return false;

    var dateA = a instanceof Date
      , dateB = b instanceof Date;
    if (dateA && dateB) return a.getTime() == b.getTime();
    if (dateA != dateB) return false;

    var regexpA = a instanceof RegExp
      , regexpB = b instanceof RegExp;
    if (regexpA && regexpB) return a.toString() == b.toString();
    if (regexpA != regexpB) return false;

    for (i = 0; i < keys.length; i++)
      if (!Object.prototype.hasOwnProperty.call(b, keys[i])) return false;

    for (i = 0; i < keys.length; i++)
      if(!equal(a[keys[i]], b[keys[i]])) return false;

    return true;
  }

  return false;
};

