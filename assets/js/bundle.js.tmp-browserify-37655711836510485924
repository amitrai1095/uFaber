}).call(this,typeof global !== "undefined" ? global : typeof self !== "undefined" ? self : typeof window !== "undefined" ? window : {})
},{"core-js/fn/regexp/escape":101,"core-js/shim":394,"regenerator-runtime/runtime":1027}],101:[function(require,module,exports){
