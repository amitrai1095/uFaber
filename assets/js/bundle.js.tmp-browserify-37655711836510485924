},{"redux":1025}],32:[function(require,module,exports){
'use strict';

Object.defineProperty(exports, "__esModule", {
  value: true
});
exports.upperSnakeCase = upperSnakeCase;
exports.camelCase = camelCase;
// Convert a space seperated string to an upper snake cased string
// e.g. "cat and dog" -> "CAT_AND_DOG"
function upperSnakeCase(string) {
  return string.split(' ').join('_').toUpperCase();
}

// Convert an underscore or space sepertated sting to a camel-cased string
// e.g. "eat RHUBARB_pie" -> "eatRhubarbPie"
function camelCase(string) {
  var words = string.toLowerCase().split(/_| /);
  return words.reduce(function (word, next) {
    return word + next[0].toUpperCase() + next.slice(1);
  });
}

